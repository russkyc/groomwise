                    <StackPanel
                        Grid.Column="0"
                        Margin="24"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                FontSize="22"
                                FontWeight="Medium"
                                Foreground="{DynamicResource inverted-lighten-2}"
                                LineHeight="32"
                                LineStackingStrategy="BlockLineHeight"
                                Text="Good " />
                            <TextBlock
                                FontSize="22"
                                FontWeight="Medium"
                                Foreground="{DynamicResource inverted-lighten-2}"
                                LineHeight="32"
                                LineStackingStrategy="BlockLineHeight"
                                Text="{Binding Date, UpdateSourceTrigger=PropertyChanged, Converter={x:Static converters:DateTimeToTimeOfDayConverter.Instance}}" />
                            <TextBlock
                                FontSize="22"
                                FontWeight="Medium"
                                Foreground="{DynamicResource inverted-lighten-2}"
                                LineHeight="32"
                                LineStackingStrategy="BlockLineHeight"
                                Text=", " />
                            <TextBlock
                                FontSize="22"
                                FontWeight="Medium"
                                Foreground="{DynamicResource inverted-lighten-2}"
                                LineHeight="32"
                                LineStackingStrategy="BlockLineHeight"
                                Text="{Binding User, UpdateSourceTrigger=PropertyChanged}" />
                        </StackPanel>
                        <TextBlock
                            Margin="0,20,0,0"
                            FontSize="34"
                            FontWeight="Bold"
                            Foreground="{DynamicResource inverted-lighten-3}"
                            LineHeight="32"
                            LineStackingStrategy="BlockLineHeight"
                            Text="Make Every Pet Grooming Appointment a Breeze"
                            TextWrapping="Wrap" />
                        <russkyc:ModernButton
                            Margin="0,35,0,0"
                            HorizontalAlignment="Left"
                            Content="Customize Widgets"
                            CornerRadius="5"
                            DefaultBackground="{DynamicResource inverted-lighten-3}"
                            DefaultForeground="{DynamicResource primary-default}"
                            FontWeight="Medium"
                            HoverBackground="{DynamicResource inverted-lighten-1}"
                            HoverForeground="{DynamicResource primary-default}"
                            Icon="{icon:MaterialIconExt Kind=Edit}"
                            IconPlacement="Left"
                            IconSize="18"
                            PressedBackground="{DynamicResource inverted-darken-1}"
                            PressedForeground="{DynamicResource primary-default}" />
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
        <Grid
            Grid.Row="2"
            Grid.Column="0"
            Margin="0,20,0,0"
            Style="{StaticResource FadeInFromBottomAnimation}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <TextBlock
                Grid.Row="0"
                Margin="0,0,0,12"
                FontSize="16"
                Text="Today" />
            <ItemsControl Grid.Row="1" ItemsSource="{Binding Appointments.EditableCollectionView, UpdateSourceTrigger=PropertyChanged}">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <templates:DashboardAppointmentTemplate />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>

                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <VirtualizingStackPanel IsVirtualizing="True" VirtualizationMode="Recycling" />
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>

                <ItemsControl.ItemContainerStyle>
                    <Style TargetType="{x:Type FrameworkElement}">
                        <Setter Property="Margin" Value="0,0,0,10" />
                    </Style>
                </ItemsControl.ItemContainerStyle>

                <ItemsControl.Template>
                    <ControlTemplate TargetType="ItemsControl">
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <ScrollViewer
                                Padding="{TemplateBinding Padding}"
                                CanContentScroll="True"
                                Focusable="False"
                                VerticalScrollBarVisibility="Auto">
                                <ItemsPresenter />
                            </ScrollViewer>
                        </Border>
                    </ControlTemplate>
                </ItemsControl.Template>
            </ItemsControl>
        </Grid>
        <Grid
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border
                Grid.Row="0"
                Margin="16,0,0,0"
                VerticalAlignment="Top"
                Background="{DynamicResource bg-100}"
                CornerRadius="10">
                <Grid MinHeight="300" HorizontalAlignment="Stretch">
                    <controls:CalendarControl Margin="4" />
                </Grid>
            </Border>
            <Grid
                Grid.Row="1"
                Margin="16,20,0,0"
                Style="{StaticResource FadeInFromBottomAnimation}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Margin="0,0,0,12"
                    FontSize="16"
                    Text="Upcoming" />
                <ItemsControl Grid.Row="1" ItemsSource="{Binding UpcomingAppointments.EditableCollectionView, UpdateSourceTrigger=PropertyChanged}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <templates:DashboardUpcomingAppointmentTemplate />
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>

                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <VirtualizingStackPanel IsVirtualizing="True" VirtualizationMode="Recycling" />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>

                    <ItemsControl.ItemContainerStyle>
                        <Style TargetType="{x:Type FrameworkElement}">
                            <Setter Property="Margin" Value="0,0,0,10" />
                        </Style>
                    </ItemsControl.ItemContainerStyle>

                    <ItemsControl.Template>
                        <ControlTemplate TargetType="ItemsControl">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                                <ScrollViewer
                                    Padding="{TemplateBinding Padding}"
                                    CanContentScroll="True"
                                    Focusable="False"
                                    VerticalScrollBarVisibility="Auto">
                                    <ItemsPresenter />
                                </ScrollViewer>
                            </Border>
                        </ControlTemplate>
                    </ItemsControl.Template>
                </ItemsControl>
            </Grid>
        </Grid>
    </Grid>
</UserControl>